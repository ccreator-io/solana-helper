{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/base58/encode",
      "methods": ["GET", "POST"],
      "dest": "/index.js"
    },
    {
      "src": "/base58/decode",
      "methods": ["GET", "POST"],
      "dest": "/index.js"
    }
  ],
  "env": {
    "PORT": "3000"
  }
}
